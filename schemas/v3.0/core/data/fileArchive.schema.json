{
  "$id": "https://openminds.ebrains.eu/core/FileArchive?format=json-schema",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": null,
  "properties": {
    "@id": {
      "description": "Metadata node identifier.",
      "type": "string"
    },
    "@type": {
      "const": "https://openminds.ebrains.eu/core/FileArchive",
      "description": "Metadata schema identifier.",
      "type": "string"
    },
    "https://openminds.ebrains.eu/vocab/IRI": {
      "description": "Stands for Internationalized Resource Identifier which is an internet protocol standard that builds on the URI protocol, extending the set of permitted characters to include Unicode/ISO 10646.",
      "format": "iri",
      "name": "IRI",
      "type": "string"
    },
    "https://openminds.ebrains.eu/vocab/format": {
      "description": "Method of digitally organizing and structuring data or information.",
      "else": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "if": {
        "required": [
          "@type"
        ]
      },
      "name": "format",
      "then": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          },
          "@type": {
            "enum": [
              "https://openminds.ebrains.eu/core/ContentType"
            ],
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "type": "object"
    },
    "https://openminds.ebrains.eu/vocab/sourceData": {
      "description": null,
      "items": {
        "else": {
          "properties": {
            "@id": {
              "format": "iri",
              "type": "string"
            }
          },
          "required": [
            "@id"
          ]
        },
        "if": {
          "required": [
            "@type"
          ]
        },
        "then": {
          "properties": {
            "@id": {
              "format": "iri",
              "type": "string"
            },
            "@type": {
              "enum": [
                "https://openminds.ebrains.eu/core/File"
              ],
              "format": "iri",
              "type": "string"
            }
          },
          "required": [
            "@id"
          ]
        },
        "type": "object"
      },
      "minItems": 1,
      "name": "sourceData",
      "type": "array",
      "uniqueItems": true
    }
  },
  "required": [
    "@id",
    "@type",
    "https://openminds.ebrains.eu/vocab/IRI",
    "https://openminds.ebrains.eu/vocab/format"
  ],
  "title": "FileArchive",
  "type": "object"
}