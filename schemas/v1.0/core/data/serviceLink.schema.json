{
  "$id": "https://openminds.ebrains.eu/core/ServiceLink?format=json-schema",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": null,
  "properties": {
    "@id": {
      "description": "Metadata node identifier.",
      "type": "string"
    },
    "@type": {
      "const": "https://openminds.ebrains.eu/core/ServiceLink",
      "description": "Metadata schema identifier.",
      "type": "string"
    },
    "https://openminds.ebrains.eu/vocab/dataLocation": {
      "description": null,
      "else": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "if": {
        "required": [
          "@type"
        ]
      },
      "name": "dataLocation",
      "then": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          },
          "@type": {
            "enum": [
              "https://openminds.ebrains.eu/core/File",
              "https://openminds.ebrains.eu/core/FileBundle"
            ],
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "type": "object"
    },
    "https://openminds.ebrains.eu/vocab/name": {
      "description": "Word or phrase that constitutes the distinctive designation of a being or thing.",
      "name": "name",
      "type": "string"
    },
    "https://openminds.ebrains.eu/vocab/openDataIn": {
      "description": null,
      "else": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "if": {
        "required": [
          "@type"
        ]
      },
      "name": "openDataIn",
      "then": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          },
          "@type": {
            "enum": [
              "https://openminds.ebrains.eu/core/URL"
            ],
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "type": "object"
    },
    "https://openminds.ebrains.eu/vocab/service": {
      "description": null,
      "else": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "if": {
        "required": [
          "@type"
        ]
      },
      "name": "service",
      "then": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          },
          "@type": {
            "enum": [
              "https://openminds.ebrains.eu/controlledTerms/Service"
            ],
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "type": "object"
    }
  },
  "required": [
    "@id",
    "@type",
    "https://openminds.ebrains.eu/vocab/dataLocation",
    "https://openminds.ebrains.eu/vocab/openDataIn",
    "https://openminds.ebrains.eu/vocab/service"
  ],
  "title": "ServiceLink",
  "type": "object"
}