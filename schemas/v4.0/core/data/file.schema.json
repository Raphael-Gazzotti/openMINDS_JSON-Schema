{
  "$id": "https://openminds.om-i.org/types/File?format=json-schema",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Structured information on a file instance that is accessible via a URL.",
  "properties": {
    "@id": {
      "description": "Metadata node identifier.",
      "type": "string"
    },
    "@type": {
      "const": "https://openminds.om-i.org/types/File",
      "description": "Metadata schema identifier.",
      "type": "string"
    },
    "https://openminds.om-i.org/props/IRI": {
      "description": "Stands for Internationalized Resource Identifier which is an internet protocol standard that builds on the URI protocol, extending the set of permitted characters to include Unicode/ISO 10646.",
      "format": "iri",
      "name": "IRI",
      "type": "string"
    },
    "https://openminds.om-i.org/props/contentDescription": {
      "description": null,
      "name": "contentDescription",
      "type": "string"
    },
    "https://openminds.om-i.org/props/dataType": {
      "description": null,
      "items": {
        "else": {
          "properties": {
            "@id": {
              "format": "iri",
              "type": "string"
            }
          },
          "required": [
            "@id"
          ]
        },
        "if": {
          "required": [
            "@type"
          ]
        },
        "then": {
          "properties": {
            "@id": {
              "format": "iri",
              "type": "string"
            },
            "@type": {
              "enum": [
                "https://openminds.om-i.org/types/DataType"
              ],
              "format": "iri",
              "type": "string"
            }
          },
          "required": [
            "@id"
          ]
        },
        "type": "object"
      },
      "minItems": 1,
      "name": "dataType",
      "type": "array",
      "uniqueItems": true
    },
    "https://openminds.om-i.org/props/fileRepository": {
      "description": null,
      "else": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "if": {
        "required": [
          "@type"
        ]
      },
      "name": "fileRepository",
      "then": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          },
          "@type": {
            "enum": [
              "https://openminds.om-i.org/types/FileRepository"
            ],
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "type": "object"
    },
    "https://openminds.om-i.org/props/format": {
      "description": "Method of digitally organizing and structuring data or information.",
      "else": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "if": {
        "required": [
          "@type"
        ]
      },
      "name": "format",
      "then": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          },
          "@type": {
            "enum": [
              "https://openminds.om-i.org/types/ContentType"
            ],
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "type": "object"
    },
    "https://openminds.om-i.org/props/hash": {
      "description": "Term used for the process of converting any data into a single value. Often also directly refers to the resulting single value.",
      "items": {
        "$ref": "https://openminds.om-i.org/types/Hash?format=json-schema",
        "type": "object"
      },
      "minItems": 1,
      "name": "hash",
      "type": "array",
      "uniqueItems": true
    },
    "https://openminds.om-i.org/props/isPartOf": {
      "description": "Reference to the ensemble of multiple things or beings.",
      "items": {
        "else": {
          "properties": {
            "@id": {
              "format": "iri",
              "type": "string"
            }
          },
          "required": [
            "@id"
          ]
        },
        "if": {
          "required": [
            "@type"
          ]
        },
        "then": {
          "properties": {
            "@id": {
              "format": "iri",
              "type": "string"
            },
            "@type": {
              "enum": [
                "https://openminds.om-i.org/types/FileBundle"
              ],
              "format": "iri",
              "type": "string"
            }
          },
          "required": [
            "@id"
          ]
        },
        "type": "object"
      },
      "minItems": 1,
      "name": "isPartOf",
      "type": "array",
      "uniqueItems": true
    },
    "https://openminds.om-i.org/props/name": {
      "description": "Word or phrase that constitutes the distinctive designation of a being or thing.",
      "name": "name",
      "type": "string"
    },
    "https://openminds.om-i.org/props/specialUsageRole": {
      "description": "Particular function of something when it is used.",
      "else": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "if": {
        "required": [
          "@type"
        ]
      },
      "name": "specialUsageRole",
      "then": {
        "properties": {
          "@id": {
            "format": "iri",
            "type": "string"
          },
          "@type": {
            "enum": [
              "https://openminds.om-i.org/types/FileUsageRole"
            ],
            "format": "iri",
            "type": "string"
          }
        },
        "required": [
          "@id"
        ]
      },
      "type": "object"
    },
    "https://openminds.om-i.org/props/storageSize": {
      "$ref": "https://openminds.om-i.org/types/QuantitativeValue?format=json-schema",
      "description": "Quantitative value defining how much disk space is used by an object on a computer system.",
      "name": "storageSize",
      "type": "object"
    }
  },
  "required": [
    "@id",
    "@type",
    "https://openminds.om-i.org/props/IRI",
    "https://openminds.om-i.org/props/name"
  ],
  "title": "File",
  "type": "object"
}